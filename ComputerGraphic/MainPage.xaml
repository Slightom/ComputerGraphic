<Page
    x:Class="ComputerGraphic.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ComputerGraphic"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:controls="using:Windows.UI.Xaml.Controls"
    xmlns:ui="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:Controls="using:Coding4Fun.Toolkit.Controls"
    xmlns:uc="clr-namespace:System.Windows.Controls"
    mc:Ignorable="d"
    Width="1366"
    Height="696"
    Background="Black"
    Foreground="White">

    <ui:Page.Resources>
        <ui:Style x:Key="MyStyleOfListBox" TargetType="ui:ListBoxItem">
            <ui:Setter Property="Background" Value="Transparent"/>
            <ui:Setter Property="TabNavigation" Value="Local"/>
            <ui:Setter Property="Padding" Value="12,11,12,13"/>
            <ui:Setter Property="HorizontalContentAlignment" Value="Left"/>
            <ui:Setter Property="UseSystemFocusVisuals" Value="True"/>
            <ui:Setter Property="Template">
                <ui:Setter.Value>
                    <ui:ControlTemplate TargetType="ui:ListBoxItem">
                        <ui:Grid x:Name="LayoutRoot" BorderThickness="{ui:TemplateBinding BorderThickness}" Background="{ui:TemplateBinding Background}">
                            <ui:Grid.Resources>
                                <ui:Style x:Key="BaseContentPresenterStyle" TargetType="ui:ContentPresenter">
                                    <ui:Setter Property="FontFamily" Value="XamlAutoFontFamily"/>
                                    <ui:Setter Property="FontWeight" Value="SemiBold"/>
                                    <ui:Setter Property="FontSize" Value="15"/>
                                    <ui:Setter Property="TextWrapping" Value="Wrap"/>
                                    <ui:Setter Property="LineStackingStrategy" Value="MaxHeight"/>
                                    <ui:Setter Property="TextLineBounds" Value="Full"/>
                                    <ui:Setter Property="OpticalMarginAlignment" Value="TrimSideBearings"/>
                                </ui:Style>
                                <ui:Style x:Key="BodyContentPresenterStyle" BasedOn="{ui:StaticResource BaseContentPresenterStyle}" TargetType="ui:ContentPresenter">
                                    <ui:Setter Property="FontWeight" Value="Normal"/>
                                    <ui:Setter Property="FontSize" Value="15"/>
                                </ui:Style>
                            </ui:Grid.Resources>
                            <ui:VisualStateManager.VisualStateGroups>
                                <ui:VisualStateGroup x:Name="CommonStates">
                                    <ui:VisualState x:Name="Normal"/>
                                    <ui:VisualState x:Name="Disabled">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlDisabledBaseMediumLowBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="PointerOver">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#585858"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="Pressed">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#ddd50a"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="Selected">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#fff72b"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="SelectedUnfocused">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#eee61a"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="SelectedPointerOver">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#fff72b"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="SelectedPressed">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="PressedBackground">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#fff72b"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SystemControlHighlightAltBaseHighBrush}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                </ui:VisualStateGroup>
                            </ui:VisualStateManager.VisualStateGroups>
                            <ui:Rectangle x:Name="PressedBackground" Fill="Transparent" ui:Control.IsTemplateFocusTarget="True"/>
                            <ui:ContentPresenter x:Name="ContentPresenter" ContentTemplate="{ui:TemplateBinding ContentTemplate}" ContentTransitions="{ui:TemplateBinding ContentTransitions}" Content="{ui:TemplateBinding Content}" HorizontalAlignment="{ui:TemplateBinding HorizontalContentAlignment}" Margin="{ui:TemplateBinding Padding}" Style="{ui:StaticResource BodyContentPresenterStyle}" TextWrapping="NoWrap" VerticalAlignment="{ui:TemplateBinding VerticalContentAlignment}"/>
                        </ui:Grid>
                    </ui:ControlTemplate>
                </ui:Setter.Value>
            </ui:Setter>
        </ui:Style>
        <ui:Style x:Key="MyButtonStyle" TargetType="ui:Button">
            <ui:Setter Property="Background" Value="{ui:ThemeResource ButtonBackground}"/>
            <ui:Setter Property="Foreground" Value="{ui:ThemeResource ButtonForeground}"/>
            <ui:Setter Property="BorderBrush" Value="{ui:ThemeResource ButtonBorderBrush}"/>
            <ui:Setter Property="BorderThickness" Value="{ui:ThemeResource ButtonBorderThemeThickness}"/>
            <ui:Setter Property="Padding" Value="8,4,8,4"/>
            <ui:Setter Property="HorizontalAlignment" Value="Left"/>
            <ui:Setter Property="VerticalAlignment" Value="Center"/>
            <ui:Setter Property="FontFamily" Value="{ui:ThemeResource ContentControlThemeFontFamily}"/>
            <ui:Setter Property="FontWeight" Value="Normal"/>
            <ui:Setter Property="FontSize" Value="{ui:ThemeResource ControlContentThemeFontSize}"/>
            <ui:Setter Property="UseSystemFocusVisuals" Value="True"/>
            <ui:Setter Property="FocusVisualMargin" Value="-3"/>
            <ui:Setter Property="Template">
                <ui:Setter.Value>
                    <ui:ControlTemplate TargetType="ui:Button">
                        <ui:Grid x:Name="RootGrid" Background="#eee61a">
                            <ui:VisualStateManager.VisualStateGroups>
                                <ui:VisualStateGroup x:Name="CommonStates">
                                    <ui:VisualState x:Name="Normal">
                                        <ui:Storyboard>
                                            <ui:PointerUpThemeAnimation ui:Storyboard.TargetName="RootGrid"/>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="PointerOver">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="RootGrid">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="#ddd50a"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="BorderBrush" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonBorderBrushPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonForegroundPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:PointerUpThemeAnimation ui:Storyboard.TargetName="RootGrid"/>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="Pressed">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="RootGrid">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonBackgroundPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="BorderBrush" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonBorderBrushPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonForegroundPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:PointerDownThemeAnimation ui:Storyboard.TargetName="RootGrid"/>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="Disabled">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="RootGrid">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonBackgroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="BorderBrush" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonBorderBrushDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="ContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource ButtonForegroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                </ui:VisualStateGroup>
                            </ui:VisualStateManager.VisualStateGroups>
                            <ui:ContentPresenter x:Name="ContentPresenter" ui:AutomationProperties.AccessibilityView="Raw" BorderBrush="{ui:TemplateBinding BorderBrush}" BorderThickness="{ui:TemplateBinding BorderThickness}" ContentTemplate="{ui:TemplateBinding ContentTemplate}" ContentTransitions="{ui:TemplateBinding ContentTransitions}" Content="{ui:TemplateBinding Content}" HorizontalContentAlignment="{ui:TemplateBinding HorizontalContentAlignment}" Padding="{ui:TemplateBinding Padding}" VerticalContentAlignment="{ui:TemplateBinding VerticalContentAlignment}"/>
                        </ui:Grid>
                    </ui:ControlTemplate>
                </ui:Setter.Value>
            </ui:Setter>
        </ui:Style>
        <ui:Style x:Key="RSlider" TargetType="ui:Slider">
            <ui:Setter Property="Background" Value="{ui:ThemeResource SliderTrackFill}"/>
            <ui:Setter Property="BorderThickness" Value="{ui:ThemeResource SliderBorderThemeThickness}"/>
            <ui:Setter Property="Foreground" Value="Red"/>
            <ui:Setter Property="FontFamily" Value="{ui:ThemeResource ContentControlThemeFontFamily}"/>
            <ui:Setter Property="FontSize" Value="{ui:ThemeResource ControlContentThemeFontSize}"/>
            <ui:Setter Property="ManipulationMode" Value="None"/>
            <ui:Setter Property="UseSystemFocusVisuals" Value="True"/>
            <ui:Setter Property="FocusVisualMargin" Value="-7,0,-7,0"/>
            <ui:Setter Property="IsFocusEngagementEnabled" Value="True"/>
            <ui:Setter Property="Template">
                <ui:Setter.Value>
                    <ui:ControlTemplate TargetType="ui:Slider">
                        <ui:Grid Margin="{ui:TemplateBinding Padding}">
                            <ui:Grid.Resources>
                                <ui:Style x:Key="SliderThumbStyle" TargetType="ui:Thumb">
                                    <ui:Setter Property="BorderThickness" Value="0"/>
                                    <ui:Setter Property="Background" Value="{ui:ThemeResource SliderThumbBackground}"/>
                                    <ui:Setter Property="Template">
                                        <ui:Setter.Value>
                                            <ui:ControlTemplate TargetType="ui:Thumb">
                                                <ui:Border BorderBrush="{ui:TemplateBinding BorderBrush}" BorderThickness="{ui:TemplateBinding BorderThickness}" Background="{ui:TemplateBinding Background}" CornerRadius="4"/>
                                            </ui:ControlTemplate>
                                        </ui:Setter.Value>
                                    </ui:Setter>
                                </ui:Style>
                            </ui:Grid.Resources>
                            <ui:Grid.RowDefinitions>
                                <ui:RowDefinition Height="Auto"/>
                                <ui:RowDefinition Height="*"/>
                            </ui:Grid.RowDefinitions>
                            <ui:VisualStateManager.VisualStateGroups>
                                <ui:VisualStateGroup x:Name="CommonStates">
                                    <ui:VisualState x:Name="Normal"/>
                                    <ui:VisualState x:Name="Pressed">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="HorizontalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="VerticalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="SliderContainer">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderContainerBackgroundPressed}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="Red"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="Red"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="Disabled">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Foreground" ui:Storyboard.TargetName="HeaderContentPresenter">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderHeaderForegroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="Red"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackValueFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="HorizontalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="VerticalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="TopTickBar">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTickBarFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="BottomTickBar">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTickBarFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="LeftTickBar">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTickBarFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="RightTickBar">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTickBarFillDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="SliderContainer">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderContainerBackgroundDisabled}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="PointerOver">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalTrackRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackFillPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="HorizontalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="VerticalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderThumbBackgroundPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Background" ui:Storyboard.TargetName="SliderContainer">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderContainerBackgroundPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="HorizontalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="Red"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="Fill" ui:Storyboard.TargetName="VerticalDecreaseRect">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="{ui:ThemeResource SliderTrackValueFillPointerOver}"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                </ui:VisualStateGroup>
                                <ui:VisualStateGroup x:Name="FocusEngagementStates">
                                    <ui:VisualState x:Name="FocusDisengaged"/>
                                    <ui:VisualState x:Name="FocusEngagedHorizontal">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="(Control.IsTemplateFocusTarget)" ui:Storyboard.TargetName="SliderContainer">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="False"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="(Control.IsTemplateFocusTarget)" ui:Storyboard.TargetName="HorizontalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="True"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                    <ui:VisualState x:Name="FocusEngagedVertical">
                                        <ui:Storyboard>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="(Control.IsTemplateFocusTarget)" ui:Storyboard.TargetName="SliderContainer">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="False"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                            <ui:ObjectAnimationUsingKeyFrames ui:Storyboard.TargetProperty="(Control.IsTemplateFocusTarget)" ui:Storyboard.TargetName="VerticalThumb">
                                                <ui:DiscreteObjectKeyFrame KeyTime="0" Value="True"/>
                                            </ui:ObjectAnimationUsingKeyFrames>
                                        </ui:Storyboard>
                                    </ui:VisualState>
                                </ui:VisualStateGroup>
                            </ui:VisualStateManager.VisualStateGroups>
                            <ui:ContentPresenter x:Name="HeaderContentPresenter" ContentTemplate="{ui:TemplateBinding HeaderTemplate}" Content="{ui:TemplateBinding Header}" Foreground="{ui:ThemeResource SliderHeaderForeground}" FontWeight="{ui:ThemeResource SliderHeaderThemeFontWeight}" Margin="{ui:ThemeResource SliderHeaderThemeMargin}" TextWrapping="Wrap" Visibility="Collapsed" x:DeferLoadStrategy="Lazy"/>
                            <ui:Grid x:Name="SliderContainer" Background="{ui:ThemeResource SliderContainerBackground}" ui:Control.IsTemplateFocusTarget="True" ui:Grid.Row="1">
                                <ui:Grid x:Name="HorizontalTemplate" MinHeight="44">
                                    <ui:Grid.ColumnDefinitions>
                                        <ui:ColumnDefinition Width="Auto"/>
                                        <ui:ColumnDefinition Width="Auto"/>
                                        <ui:ColumnDefinition Width="*"/>
                                    </ui:Grid.ColumnDefinitions>
                                    <ui:Grid.RowDefinitions>
                                        <ui:RowDefinition Height="18"/>
                                        <ui:RowDefinition Height="Auto"/>
                                        <ui:RowDefinition Height="18"/>
                                    </ui:Grid.RowDefinitions>
                                    <ui:Rectangle x:Name="HorizontalTrackRect" ui:Grid.ColumnSpan="3" Fill="{ui:TemplateBinding Background}" Height="4" ui:Grid.Row="1"/>
                                    <ui:Rectangle x:Name="HorizontalDecreaseRect" Fill="{ui:TemplateBinding Foreground}" ui:Grid.Row="1"/>
                                    <ui:TickBar x:Name="TopTickBar" ui:Grid.ColumnSpan="3" Fill="{ui:ThemeResource SliderTickBarFill}" Height="{ui:ThemeResource SliderOutsideTickBarThemeHeight}" Margin="0,0,0,4" Visibility="Collapsed" VerticalAlignment="Bottom"/>
                                    <ui:TickBar x:Name="HorizontalInlineTickBar" ui:Grid.ColumnSpan="3" Fill="{ui:ThemeResource SliderInlineTickBarFill}" Height="{ui:ThemeResource SliderTrackThemeHeight}" ui:Grid.Row="1" Visibility="Collapsed"/>
                                    <ui:TickBar x:Name="BottomTickBar" ui:Grid.ColumnSpan="3" Fill="{ui:ThemeResource SliderTickBarFill}" Height="{ui:ThemeResource SliderOutsideTickBarThemeHeight}" Margin="0,4,0,0" ui:Grid.Row="2" Visibility="Collapsed" VerticalAlignment="Top"/>
                                    <ui:Thumb x:Name="HorizontalThumb" ui:AutomationProperties.AccessibilityView="Raw" ui:Grid.Column="1" DataContext="{ui:TemplateBinding Value}" Height="24" ui:Grid.Row="0" ui:Grid.RowSpan="3" Style="{ui:StaticResource SliderThumbStyle}" Width="8"/>
                                </ui:Grid>
                                <ui:Grid x:Name="VerticalTemplate" MinWidth="44" Visibility="Collapsed">
                                    <ui:Grid.ColumnDefinitions>
                                        <ui:ColumnDefinition Width="18"/>
                                        <ui:ColumnDefinition Width="Auto"/>
                                        <ui:ColumnDefinition Width="18"/>
                                    </ui:Grid.ColumnDefinitions>
                                    <ui:Grid.RowDefinitions>
                                        <ui:RowDefinition Height="*"/>
                                        <ui:RowDefinition Height="Auto"/>
                                        <ui:RowDefinition Height="Auto"/>
                                    </ui:Grid.RowDefinitions>
                                    <ui:Rectangle x:Name="VerticalTrackRect" ui:Grid.Column="1" Fill="{ui:TemplateBinding Background}" ui:Grid.RowSpan="3" Width="{ui:ThemeResource SliderTrackThemeHeight}"/>
                                    <ui:Rectangle x:Name="VerticalDecreaseRect" ui:Grid.Column="1" Fill="{ui:TemplateBinding Foreground}" ui:Grid.Row="2"/>
                                    <ui:TickBar x:Name="LeftTickBar" Fill="{ui:ThemeResource SliderTickBarFill}" HorizontalAlignment="Right" Margin="0,0,4,0" ui:Grid.RowSpan="3" Visibility="Collapsed" Width="{ui:ThemeResource SliderOutsideTickBarThemeHeight}"/>
