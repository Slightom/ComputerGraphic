                        <TextBlock Name="X1TextBlock" Text="X1" FontSize="14" VerticalAlignment="Center" Margin="10,0,5,0"/>
                        <TextBox Name="X1TextBox" FontSize="14" Height="10" Width="80"/>
                        <TextBlock Name="Y1TextBlock" Text="Y1" FontSize="14" VerticalAlignment="Center" Margin="10,0,5,0"/>
                        <TextBox Name="Y1TextBox" FontSize="14" Height="10" Width="80"/>

                        <TextBlock Name="X2TextBlock" Text="X2" FontSize="14" VerticalAlignment="Center" Margin="30,0,5,0"/>
                        <TextBox Name="X2TextBox" FontSize="14" Height="10" Width="80"/>
                        <TextBlock Name="Y2TextBlock" Text="Y2" FontSize="14" VerticalAlignment="Center" Margin="10,0,5,0"/>
                        <TextBox Name="Y2TextBox" FontSize="14" Height="10" Width="80"/>

                        <Button Name="ConfirmDimensions" Content="Confirm" FontSize="14" Margin="30,0,0,0" Click="ConfirmDimensions_Click" Background="LightGray"
                                BorderBrush="Black" BorderThickness="2" Style="{ui:StaticResource MyButtonStyle}"/>
                    </StackPanel>

                </StackPanel>
            </CommandBar.Content>
            <CommandBar.PrimaryCommands>
                <!--<AppBarButton Icon="Add"
                              Label="Add" />
                <AppBarButton Icon="Remove"
                              Label="Remove" />-->
            </CommandBar.PrimaryCommands>
            <CommandBar.SecondaryCommands>
                <AppBarButton Label="Load file" Click="LoadFileClickAsync"/>
                <AppBarButton Label="Modify image"/>
            </CommandBar.SecondaryCommands>
        </CommandBar>


        <Grid Grid.Row="1">

            <Popup Name="ColorPickerPopup" IsOpen="False">
                <StackPanel Background="DarkGray" Margin="5,5,0,0" Orientation="Vertical">
                    <Controls:ColorPicker x:Name="ColorPicker" Height="258" Width="374" Margin="5, 5, 5, 15"
                                                      ColorChanged="Colorpick_ColorChanged" Visibility="Visible"/>
                    <StackPanel Orientation="Vertical" Margin="9,0,5,5" >
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="RTextBlock" Text="R" Width="30" TextWrapping="Wrap" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="RTextBox" Height="32" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="RSlider" Width="200" Maximum="255" Minimum="0" VerticalAlignment="Center" ValueChanged="SliderRGB_ValueChanged" Style="{ui:StaticResource RSlider}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="GTextBlock" Text="G" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="GTextBox" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="GSlider" Width="200" Maximum="255" Minimum="0" VerticalAlignment="Center" ValueChanged="SliderRGB_ValueChanged" Style="{ui:StaticResource GSlider}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="BTextBlock" Text="B" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="BTextBox" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False"  Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="BSlider" Width="200" Maximum="255" Minimum="0" VerticalAlignment="Center" ValueChanged="SliderRGB_ValueChanged" Style="{ui:StaticResource BSlider}"/>
                        </StackPanel>

                        <Border BorderBrush="Gray" BorderThickness="1" Width="292" Margin="5,15" HorizontalAlignment="Left"/>

                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="CTextBlock" Text="C" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="CTextBox"  FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="CSlider" Width="200" Maximum="1" Minimum="0" StepFrequency="0.0001" VerticalAlignment="Center" ValueChanged="SliderCMYK_ValueChanged" Style="{ui:StaticResource CSlider}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="MTextBlock" Text="M" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="MTextBox" FontSize="16"  HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0"  IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="MSlider" Width="200" Maximum="1" Minimum="0" StepFrequency="0.0001" VerticalAlignment="Center" ValueChanged="SliderCMYK_ValueChanged" Style="{ui:StaticResource MSlider}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="YTextBlock" Text="Y" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="YTextBox" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="YSlider" Width="200" Maximum="1" Minimum="0" StepFrequency="0.0001" VerticalAlignment="Center" ValueChanged="SliderCMYK_ValueChanged" Style="{ui:StaticResource YSlider}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Width="369" VerticalAlignment="Center">
                            <TextBlock x:Name="KTextBlock" Text="K" Width="30" FontSize="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox x:Name="KTextBox" FontSize="16" VerticalAlignment="Center" Margin="0,0,5,0" IsEnabled="False" Style="{ui:StaticResource MyTextBoxDisable}"/>
                            <Slider x:Name="KSlider" Width="200" Maximum="1.00" Minimum="0.00" StepFrequency="0.0001" VerticalAlignment="Center" ValueChanged="SliderCMYK_ValueChanged" Style="{ui:StaticResource KSlider}"/>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </Popup>

            <Popup Name="ImageManipulator" IsOpen="True" HorizontalAlignment="Right" Margin="0,5,420,0">
                <StackPanel Background="DarkGray" Orientation="Vertical">
                    <StackPanel Orientation="Vertical" Margin="9,10,5,5" >
                        <StackPanel Orientation="Horizontal" Width="400" VerticalAlignment="Center">
                            <TextBlock x:Name="Adding" Text="Adding" Width="100" TextWrapping="Wrap" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-7,5,0"/>
                            <Slider x:Name="AddingSlider" Width="200" Maximum="255" Minimum="-255" Value="0" VerticalAlignment="Center" ValueChanged="AddingSlider_ValueChanged" IsThumbToolTipEnabled="False"/>
                            <TextBox x:Name="AddTextBox" Height="32" Width="70" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,-7,5,0" IsEnabled="True" TextChanged="AddingTextBoxChanged"/>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Width="400" VerticalAlignment="Center">
                            <TextBlock x:Name="Multiplying" Text="Multiplying" Width="100" TextWrapping="Wrap" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-7,5,0"/>
                            <Slider x:Name="MultiplyingSlider" Width="200" Maximum="255" Minimum="-255" StepFrequency="0.1" Value="0" VerticalAlignment="Center" ValueChanged="MultiplySlider_ValueChanged" IsThumbToolTipEnabled="False"/>
                            <TextBox x:Name="MultiplyTextBox" Height="32" Width="70" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,-7,5,0" IsEnabled="True" TextChanged="MultiplyTextBoxChanged"/>
                        </StackPanel>

                        <Border BorderBrush="Gray" BorderThickness="1" Width="292" Margin="5,15" HorizontalAlignment="Left"/>
                    </StackPanel>
                </StackPanel>
            </Popup>


            <ScrollViewer ZoomMode="Enabled" MinZoomFactor="0.1" MaxZoomFactor="20"
                          HorizontalScrollBarVisibility="Visible" VerticalAlignment="Stretch" 
                          HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Visible" 
                          Height="auto" Width="1366">
                <Border Height="4000" Width="8000">
                    <Canvas Name="MyCanvas"  Background="#fff72b" Height="4000" Width="8000"
                        AllowDrop="True" DragOver="Shape_DragOver" Drop="Shape_Drop" Margin="-31,0,-33,10">
                    </Canvas>
                </Border>
            </ScrollViewer>
        </Grid>

    </Grid>

</Page>
